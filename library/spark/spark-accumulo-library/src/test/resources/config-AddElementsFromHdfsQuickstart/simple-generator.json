{
  "class" : "CsvElementGenerator",
  "header" : [ "src", "dest", "timestamp" ],
  "allFieldsRequired" : true,
  "csvTransforms" : [

    {
    "selection" : [ ],
    "function" : {
      "class" : "SetValue",
      "value" : 1
    },
    "projection" : [ "count" ]
  },

    {
    "selection" : [ "count" ],
    "function" : {
      "class" : "uk.gov.gchq.koryphe.impl.function.ToLong"
    },
    "projection" : [ "count" ]
  }

  ],
  "elements" : [

    {
    "GROUP" : "Edge",
    "DESTINATION" : "dest",
    "SOURCE" : "src",
    "DIRECTED" : true,
    "count" : "count"
  },

    {
    "GROUP" : "Entity",
    "VERTEX" : "src",
    "count" : "count"
  },

    {
    "GROUP" : "Entity",
    "VERTEX" : "dest",
    "count" : "count"
  }

  ]
}
